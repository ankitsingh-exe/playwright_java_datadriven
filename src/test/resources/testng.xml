<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Open Cart Test Regression Playwright Suite" verbose="4"  parallel="tests" thread-count="5">

    <listeners>
        <listener class-name="com.company.listeners.ExtendReportListeners"/>
    </listeners>
    
    <test name="Open Cart Home Page Test">
        <classes>
            <class name="com.company.opencart.tests.HomePageTest"/>
        </classes>
    </test>

    <test name="Open Cart Login Page Test">
        <classes>
            <class name="com.company.opencart.tests.LoginPageTest"/>
        </classes>
    </test>

</suite>
